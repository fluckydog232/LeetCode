//          [5,3,1,6,7,8]
//min        [5,3,1,1,1]
//profit      [0,0,0,5,6,7] 


class Solution {
    public static int maxProfit(int[] prices) {
    if(prices==null||prices.length<2){return 0;}
        int min=prices[0];
        int profit=0;
        for(int price:prices){
            min=Math.min(min,price);
            profit=Math.max(profit,price-min);
  
        }
        return profit;
    }
}


