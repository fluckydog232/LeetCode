class Solution {
    public int maxProfit(int[] prices) {
    if(prices==null||prices.length<2){return 0;}
 
        int maxprofit=0;
        for(int j=0;j<prices.length-1;j++){
           maxprofit+=Math.max(prices[j+1]-prices[j],0);
             }
         
        return maxprofit;
        }
        
        
    }
